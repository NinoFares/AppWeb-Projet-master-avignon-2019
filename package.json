    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "nodemailer": "^6.2.1",
    "redux-logger": "^3.0.6",
    "sha1": "^1.1.1"
  }
